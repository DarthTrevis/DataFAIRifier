@prefix uo:     <http://purl.obolibrary.org/obo/>.



############################################
# Patient ID query
############################################
map:patient-ids
    rr:sqlQuery """select id from "public"."thunder" """.


############################################
# Gender
############################################
map:genders a rr:TriplesMap;
    rr:logicalTable [
        rr:sqlQuery """select id, gender_class from "public"."thunder_view" """ ;
    ];
    rr:subjectMap [
        rr:template "http://www.cancerdata.org/roo/gender_{id}";
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:column "gender_class";
            rr:termType rr:IRI;
        ];
    ].


