############################################
# Patient
############################################
map:patients a rr:TriplesMap;
    rr:logicalTable map:patient-ids;
    rr:subjectMap [
        rr:template "http://www.cancerdata.org/roo/patient_{id}";
        rr:class ncit:C16960;
    ];
    # Disease
    rr:predicateObjectMap [
        rr:predicate roo:100008;
        rr:objectMap [
            rr:parentTriplesMap map:diseases;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];
    # Gender
    rr:predicateObjectMap [
        rr:predicate roo:100018;
        rr:objectMap [
            rr:parentTriplesMap map:genders;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100017;
        rr:objectMap [
            rr:parentTriplesMap map:age_start_age_object;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100016;
        rr:objectMap [
            rr:parentTriplesMap map:age_diag_age_object;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100218;
        rr:objectMap [
            rr:parentTriplesMap map:patientEcogResource;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100301;
        rr:objectMap [
            rr:parentTriplesMap map:sbrtResource;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ]
    ].

