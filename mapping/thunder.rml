############################################
# Disease
############################################
map:disease-time
    rr:sqlQuery """select "id", "year_intake", "month_intake", "day_intake" from "public"."thunder_view" where ("Institute" = 1)""" .

map:diseaseDateTime a rr:TriplesMap;
    rr:logicalTable map:disease-time;
    rr:subjectMap [
        rr:template "http://www.cancerdata.org/roo/diseaseDateTime_{id}";
        rr:class time:DateTimeDescription;
    ];
    rr:predicateObjectMap [
        rr:predicate time:day;
        rr:objectMap [
            rr:column "day_intake";
            rr:termType rr:Literal;
            rr:datatype <http://www.w3.org/2001/XMLSchema#string>
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate time:month;
        rr:objectMap [
            rr:column "month_intake";
            rr:termType rr:Literal;
            rr:datatype <http://www.w3.org/2001/XMLSchema#string>
         ];
    ];
    rr:predicateObjectMap [
        rr:predicate time:year;
        rr:objectMap [
            rr:column "year_intake";
            rr:termType rr:Literal;
            rr:datatype <http://www.w3.org/2001/XMLSchema#string>   
        ];
    ].

map:diseases a rr:TriplesMap;
    rr:logicalTable map:disease-time;
    rr:subjectMap [
        rr:template "http://www.cancerdata.org/roo/disease_{id}";
        rr:class icd:C20;
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100041;
        rr:objectMap [
            rr:parentTriplesMap map:diseaseDateTime;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100038; #is_when
        rr:objectMap [
            rr:parentTriplesMap map:patientEcogResource;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100243;
        rr:objectMap [
            rr:parentTriplesMap map:clinTnmResource;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100287;
        rr:objectMap [
            rr:parentTriplesMap map:pathTnmResource;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100402;
        rr:objectMap [
            rr:parentTriplesMap map:sbrtResource;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ]
    ].

############################################
# Age at start treatment
############################################
map:age-treatment-view
    rr:sqlQuery """select id, age_at_start_rt from thunder_view""" .

map:age_start_age_unit a rr:TriplesMap;
    rr:logicalTable map:age-treatment-view;
    rr:subjectMap [
        rr:template "http://www.cancerdata.org/roo/ageStartRtUnit_{id}";
        rr:class uo:UO_0000036;
    ];
    rr:predicateObjectMap [
        rr:predicate roo:100042;
        rr:objectMap [ rr:column "age_at_start_rt" ];
    ].

map:age_start_age_object a rr:TriplesMap;
    rr:logicalTable map:age-treatment-view;
    rr:subjectMap [
        rr:template "http://www.cancerdata.org/roo/ageStartRT_{id}";
